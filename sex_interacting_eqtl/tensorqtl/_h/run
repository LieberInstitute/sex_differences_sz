expression_bed='/ceph/users/jbenja13/github_projects/sex_differences_sz/prep_eqtl_analysis/caudate/genes/prepare_expression/_m/genes.expression.bed.gz'

prefix='libd_caudate'

covariates_file='/ceph/users/jbenja13/github_projects/sex_differences_sz/prep_eqtl_analysis/caudate/genes/covariates/_m/genes.combined_covariates.txt'

plink_prefix_path='../../plink/_m/libd_caudate'

interaction_file='/ceph/users/jbenja13/github_projects/sex_differences_sz/prep_eqtl_analysis/caudate/genes/_m/sex_interaction_list.txt'


python3 -m tensorqtl ${plink_prefix_path} ${expression_bed} ${prefix} \
    --window 500000 \
    --interaction ${interaction_file} \
    --maf_threshold 0.05 \
    --maf_threshold_interaction 0.05 \
    --covariates ${covariates_file} \
    --mode cis_nominal
